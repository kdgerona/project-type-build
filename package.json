{
  "name": "@types/gorentals",
  "version": "1.0.0",
  "description": "GoRentals Project Typings Schema Builder",
  "main": "dist/build",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "format": "prettier src/**/*.ts --ignore-path ./.prettierignore --write",
    "build:schema": "ts-node src/index.ts && npm run format && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.dnamicro.net/Platform6/project-level-schema-builder.git"
  },
  "publishConfig": {
    "registry": "https://npm.dnamicro.net"
  },
  "keywords": [
    "schema",
    "builder"
  ],
  "author": "Kevin Dave Gerona",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/node": "^16.11.11",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@dnamicro/gorentals-schema-core": "git+http://gitlab.dnamicro.net/Platform6-Modules/companyschemas/gorentals/core.git#company-schemas/gorentals/core/v2"
  },
  "dependencies": {
    "@dnamicro/gorentals-schema-core": "git+http://gitlab.dnamicro.net/Platform6-Modules/companyschemas/gorentals/core.git#company-schemas/gorentals/core/v2",
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "bluebird": "^3.7.2",
    "graphql": "^16.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0"
  }
}
